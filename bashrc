lias ll="ls -lh"

# 能够少输入很多
alias v="vim"

# 手误打错命令也没关系
alias sl=ls

# 重新定义一些命令行的默认行为
alias mv="mv -i"           # -i prompts before overwrite
alias mkdir="mkdir -p"     # -p make parent dirs as needed

alias gl="git log --all --graph --decorate --oneline"
alias gs="git status"
alias gc="git commit"
