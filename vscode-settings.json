{
    "files.autoSave": "onFocusChange",
    "editor.linkedEditing": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "liveServer.settings.donotShowInfoMsg": true,
    "prettier.semi": false,
    "workbench.iconTheme": "vscode-icons",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "workbench.colorTheme": "Luke Dark",
    "editor.fontSize": 16,
    "window.zoomLevel": -1,
    
    "python.formatting.provider": "yapf",
    "security.workspace.trust.untrustedFiles": "open",
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "python.defaultInterpreterPath": "C:\\Users\\夏宏超\\AppData\\Local\\Programs\\Python\\Python39\\python.exe",
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell"
        },
        "Command Prompt": {
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ],
            "args": [],
            "icon": "terminal-cmd"
        },
        "Git Bash": {
            "source": "Git Bash"
        }
    },
    "[html]": {
        "editor.tabSize": 2,
        "editor.detectIndentation": false
    },
    "[javascript]": {
        "editor.tabSize": 2,
        "editor.detectIndentation": false
    },
    "editor.tabCompletion": "on",

    "compilerPath": "D:/Program Files (x86)/mingw64/bin/gcc.exe",
    "C_Cpp.default.includePath": [
        "${workspaceFolder}/**",
        "D:/Program Files (x86)/mingw64/x86_64-w64-mingw32/include",
        "D:/Program Files (x86)/mingw64/x86_64-w64-mingw32/**"
    ],
    "C_Cpp.default.intelliSenseMode": "linux-gcc-x64",
    "[c]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, IndentWidth: 4, TabWidth: 4 }",
    "[python]": {
        "editor.defaultFormatter": "ms-python.python"
    },
    "emmet.extensionsPath": [
        "C:\\Users\\Joiy"
    ],
    "redhat.telemetry.enabled": true,
    "[xml]": {
        "editor.defaultFormatter": "redhat.vscode-xml"
    },
    "[java]": {
        "editor.defaultFormatter": "redhat.java"
    }
}